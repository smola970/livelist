<h1>Create New goal</h1>

<%= form_for @goal do |f| %>
  <p>
    <%= f.label :text %>
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.label :category_id %>
    <%= f.collection_select(:category_id, Category.all, :id, :name) %>
  </p>
  <button>Submit</button>
<% end %>
<p><%= link_to 'back', :back %></p>
