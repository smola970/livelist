<h1>Browse</h1>

<p>Goals To do:</p>
<% @goals_to_do.each do |goal| %>
  <p><%= goal.text %></p>
  <p><%= link_to 'add', "/goals/#{goal.id}/clone" %></p>
<% end %>

<p>Goals Completed:</p>
<% @goals_completed.each do |goal| %>
  <img src="<%= goal.image_url %>" alt="">
  <p><%= goal.text %></p>
  <p><%= link_to 'show', goal %></p>
  <p><%= link_to 'add', "/goals/#{goal.id}/clone" %></p>
<% end %>